# Environment Configuration
# Copy this file to .env and fill in your values

# MongoDB
MONGODB_URI=mongodb://localhost:27017/academic-dashboard

# Google Calendar OAuth (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3001/api/calendar/callback

# Canvas LMS (get from your Canvas account settings)
CANVAS_API_URL=https://your-school.instructure.com
CANVAS_ACCESS_TOKEN=your-canvas-access-token

# Session Secret (generate a random string)
SESSION_SECRET=your-session-secret-change-this
